    //     "addr": { "0x71aaaaaaaa": {hookoptions} } // (json cant do hex) Perhaps ur hook doesn't have a good name yet and u wanna fuck around at runtime
    // },

    // TODO embed or sideload/manifest scripts

    "4":20 // useless final json entry, so previous entries can always use comma. Beware of trailing commas! Also remember numbers are not valid json keys!
